{
  "compilerOptions": {
    "jsx": "react",
    "allowJs":true,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "build"
  },
  "include": [
    "src/**/*","public"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
